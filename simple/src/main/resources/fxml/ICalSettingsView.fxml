<GridPane hgap="10" vgap="10" styleClass="root"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="com.github.drbookings.ui.controller.ICalSettingsController">

	<stylesheets>
		<URL value="@/css/root.css" />
	</stylesheets>
	<padding>
		<Insets bottom="10" left="10" right="10" top="10" />
	</padding>
	<children>
		<Label GridPane.columnIndex="0" GridPane.rowIndex="0"
			GridPane.columnSpan="2" text="Mapping: Vendor Room Name -> DrBookings Room Number" />

		<TextField fx:id="vendorRoomName1" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="1" />

		<TextField fx:id="ourRoomName1" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="1" />

		<TextField fx:id="vendorRoomName2" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="2" />

		<TextField fx:id="ourRoomName2" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="2" />

		<TextField fx:id="vendorRoomName3" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="3" />

		<TextField fx:id="ourRoomName3" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="3" />

		<TextField fx:id="vendorRoomName4" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="4" />

		<TextField fx:id="ourRoomName4" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="4" />
			
			<TextField fx:id="vendorRoomName5" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="5" />

		<TextField fx:id="ourRoomName5" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="5" />
			
			<TextField fx:id="vendorRoomName6" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="6" />

		<TextField fx:id="ourRoomName6" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="6" />
			
			<TextField fx:id="vendorRoomName7" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="7" />

		<TextField fx:id="ourRoomName7" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="7" />
			
			<TextField fx:id="vendorRoomName8" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="0" GridPane.rowIndex="8" />

		<TextField fx:id="ourRoomName8" GridPane.hgrow="ALWAYS"
			GridPane.columnIndex="1" GridPane.rowIndex="8" />
			
			<Button text="Save" GridPane.columnIndex="1" GridPane.rowIndex="9" onAction="#handleSave"></Button>

	</children>



</GridPane>
