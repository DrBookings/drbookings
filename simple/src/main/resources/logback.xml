<configuration>

	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss} %-5level [%-5thread] (%file:%line\): %message%n
			</pattern>
		</encoder>
	</appender>

	

	<root level="debug">
		<appender-ref ref="stdout" />
	
	</root>

	



</configuration>
